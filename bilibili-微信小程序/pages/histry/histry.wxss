

checkbox{
  margin: 30rpx 10rpx 30rpx 30rpx;
  color:white;
}


  checkbox .wx-checkbox-input{
    border-radius: 50%;/* 圆角 */
    border: 1rpx solid #aaa; /* 边框颜色*/
 }
 /* 选中后的--样式更改 */
 checkbox .wx-checkbox-input.wx-checkbox-input-checked{
    border: none;/*无边框*/
    background-color: #FB7299;/*背景色红色*/
 }
 /* 选中后的--对勾的样式更改 （可根据自己需求修改） */
 checkbox .wx-checkbox-input.wx-checkbox-input-checked::before{
    height: 45rpx;
    width: 45rpx;
    line-height: 45rpx;
    border-radius: 50%;/* 圆角 */
    text-align: center;/* 文字居中显示*/
    font-size:35rpx; /* 对勾大小 30rpx */
    color:#fff; /* 对勾颜色 白色 */

 }

