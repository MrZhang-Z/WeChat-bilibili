}
.Mvmsg{
  height:200rpx;
   width: 100%;
  
  display: flex;
  align-items: center;
  margin-left: 20rpx;
}
.Mvmsg image{
  width: 38%;
  height: 86%;

  float: left;
  border-radius: 15rpx;
  margin-right: 15rpx;

}
.Mvtxt{
  height: 86%;
  display: flex;
  flex-direction: column;
  justify-content: space-between;

}
.Mvtxt .Mvtxt-txt{
  font-size: 30rpx;
  font-weight: bold;
  width: 300rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.Mvtxt view{
  display: flex;
  flex-direction: column;
  color: rgb(128, 126, 126);
  font-size: 25rpx;
}

